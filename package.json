{
  "name": "streamvi-sdk",
  "version": "1.0.1",
  "description": "Library for working with the StreamVi API (https://dev.streamvi.io)",
  "keywords": [
    "streamvi",
    "api",
    "sdk",
    "typescript",
    "nodejs",
    "rest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/StreamVi/streamvi-sdk.git"
  },
  "author": "StreamVi <info@streamvi.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StreamVi/streamvi-sdk/issues"
  },
  "homepage": "https://dev.streamvi.io",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "main": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "example": "ts-node example/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:preprocessing": "jest tests/clean-openapi-spec.test.ts",
    "test:postprocessing": "jest tests/postprocess-api.test.ts",
    "gen-process": "node scripts/clean-openapi-spec.js && npx @openapitools/openapi-generator-cli generate && node scripts/postprocess-api.js",
    "gen:api-prod": "node ./scripts/openapi-fetcher.mjs napi.streamvi.io && node scripts/clean-openapi-spec.js && npx @openapitools/openapi-generator-cli generate && node scripts/postprocess-api.js",
    "build": "rimraf dist && tsc",
    "build:docs": "typedoc",
    "lint": "eslint \"src/**/*.ts\"",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm run lint",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "passport": "^0.7.0",
    "passport-streamvi": "^1.0.6"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.20.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.22",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/passport": "^1.0.17",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "glob": "^11.0.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  }
}
